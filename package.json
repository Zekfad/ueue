{
	"type": "module",
	"main": "./dist/cjs/bundle.cjs",
	"module": "./dist/esm/bundle.js",
	"exports": {
		".": {
			"import": "./dist/esm/bundle.mjs",
			"require": "./dist/cjs/bundle.cjs",
			"default": "./dist/esm/bundle.cjs"
		}
	},
	"name": "ueue",
	"version": "1.0.0",
	"description": "Queue - ueue is silent. Simple event-driven queue manager.",
	"keywords": [
		"queue",
		"order",
		"async",
		"events"
	],
	"homepage": "https://zekfad.github.io/ueue/",
	"bugs": {
		"url": "https://github.com/Zekfad/ueue/issues",
		"email": "zekfad@znnme.eu.org"
	},
	"license": "ISC",
	"author": "Zekfad <zekfad@znnme.eu.org> (https://zekfad.znnme.eu.org)",
	"repository": "github:Zekfad/ueue",
	"types": "ueue.d.ts",
	"scripts": {
		"lint": "eslint . --ext .js,.cjs,.mjs",
		"dev": "rollup -c --watch --environment mode:dev",
		"build": "run-p build:**",
		"build:src": "rollup -c",
		"build:docs": "jsdoc -c .jsdoc.json --verbose",
		"build:def": "jsdoc -c .jsdoc.json --verbose -t node_modules/tsd-jsdoc/dist -d .",
		"test": "nyc --reporter=lcov mocha",
		"coverage": "codecov"
	},
	"devDependencies": {
		"@babel/core": "^7.11.0",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@rollup/plugin-babel": "^5.1.0",
		"@rollup/plugin-commonjs": "^15.0.0",
		"@rollup/plugin-json": "^4.0.2",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@zekfad/eslint-config": "^1.0.0",
		"babel-eslint": "^10.1.0",
		"builtin-modules": "^3.1.0",
		"codecov": "^3.7.2",
		"eslint": "^7.7.0",
		"eslint-import-resolver-nuxt": "^1.0.1",
		"eslint-plugin-import": "^2.22.0",
		"jsdoc": "^3.6.5",
		"magic-string": "^0.25.7",
		"mocha": "^8.1.0",
		"npm-run-all": "^4.1.5",
		"nyc": "^15.1.0",
		"rollup": "^2.23.0",
		"rollup-plugin-terser": "^7.0.0",
		"tsd-jsdoc": "^2.5.0",
		"tui-jsdoc-template": "^1.2.2"
	},
	"engines": {
		"node": ">=10"
	},
	"dependencies": {
		"uuid": "^8.3.0"
	}
}
